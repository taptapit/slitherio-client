{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Wing Player",
			"type": "chrome",
			"request": "launch",
			"file": "index.html",
			"runtimeExecutable": "${execPath}",
			//"runtimeExecutable": "C:/Program Files (x86)/Google/Chrome/Application/chrome.exe",
			"useBuildInServer": true,
			"sourceMaps": true,
			"webRoot": "${workspaceRoot}",
			"preLaunchTask":"build",
			"port":5914
		},
		{
			"name": "Launch Chrome",
			"type": "chrome",
			"request": "launch",
			"file": "index.html",
			"useBuildInServer": true,
			"sourceMaps": true,
			"webRoot": "${workspaceRoot}",
			"preLaunchTask":"build",
			"userDataDir":"${tmpdir}",
			"port":5914
		}
	]
}